{
  "name": "@frontend-forge/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "files": ["dist", "vendor"],
  "description": "KubeSphere plugin build service (SystemJS) powered by Fastify + esbuild + SWC + Tailwind.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prestart": "pnpm run build",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "project:debug": "node --import ./node_modules/tsx/dist/esm/index.mjs src/projectCli.ts"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/static": "^9.0.0",
    "fastify": "^5.6.2",
    "lru-cache": "^11.0.2",
    "p-queue": "^9.1.0",
    "undici": "^6.21.0",
    "@frontend-forge/forge-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
